Site_ID == "DNE" | Site_ID == "DSW" ~ "DMP",
# Ningaloo
Site_ID == "NGN"| Site_ID == "NGS" ~ "NMP",
# Two Rocks
Site_ID == "TRE" | Site_ID == "TRW" ~ "TRMP",
# Jurien
Site_ID == "JSE" | Site_ID == "JNE" ~ "JMP",
# Geographe
Site_ID == "GEO" ~ "GMP",
