TRUE ~ "South-west"
),
npz_id = case_when(
# Cod Grounds
Site_ID == "CG" ~ "CGMP",
# Solitary Islands
Site_ID == "EP" | Site_ID == "WP" ~ "SIMP",
# Dampier
Site_ID == "DNE" | Site_ID == "DSW" ~ "DMP",
